@model IEnumerable<Employee_smart_door_sys.Models.ep_Table>

@{
    ViewBag.Title = "employee_list";
}

<h2>employee_list</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.ep_Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ep_Img)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ep_Inday)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.ep_Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ep_Img)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ep_Inday)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.ep_Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.ep_Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.ep_Id })
        </td>
    </tr>
}

</table>
